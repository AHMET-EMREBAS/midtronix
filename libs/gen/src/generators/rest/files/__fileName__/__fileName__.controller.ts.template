import { CreateBasicController, RestRouteBuilder } from '@mdtx/core';
import {
  <%- className %>,
  Create<%- className %>Dto,
  Udpate<%- className %>Dto,
  <%- className %>QueryDto
} from '@mdtx/entities';
import { <%- className %>Service } from './<%- fileName %>.service';

import { ApiVersion } from '@mdtx/common';

export const <%- className %>RRB = RestRouteBuilder.get(<%- className %>.name, ApiVersion.v1);

@<%- className %>RRB.Controler()
export class <%- className %>Controller extends CreateBasicController<
  <%- className %>,
  Create<%- className %>Dto,
  Udpate<%- className %>Dto,
  <%- className %>QueryDto
>(<%- className %>RRB, <%- className %>, Create<%- className %>Dto, Udpate<%- className %>Dto, <%- className %>QueryDto) {
  constructor(service: <%- className %>Service) {
    super(service);
  }
}
